{
  "name": "await-statuses",
  "version": "0.0.0",
  "description": "Wait for statuses on a given ref",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build lib/main.js && sed -i 's/\\x0D$//' ./dist/index.js",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freckle/await-statuses-action.git"
  },
  "author": "Freckle",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@actions/glob": "^0.4.0",
    "@octokit/types": "^12.4.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.11.0",
    "@vercel/ncc": "^0.38.1",
    "jest": "^27.4.7",
    "prettier": "^3.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.9.5",
    "eslint": "^8.56.0"
  }
}
